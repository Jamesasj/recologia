#!/usr/bin/env python
'''

'''

import itertools


def possibilidades(seq,n):
    saida=[]
    for i in range(2,n):
        for j in range(0,n-1):
            for k in itertools.combinations(seq[j:],i):
                saida.append(list(k))           
    return saida 

if __name__ == '__main__':
    #n = 5
    #seq = [5,1,4,2,3]

    file = open( "/mnt/B21AA1BD1AA17ECB/downloads_chromium_ubuntu/rosalind_lgis.txt" )
    n=int(file.readline())
    seq=map(int, file.readline().split())
    file.close()

    print n
    print seq
    
    lista=possibilidades(seq,n)


    maior=[]
    menor=[]
    for sequencia in lista:
        if sorted(sequencia) == sequencia and len(sequencia)>len(maior):
            maior=sequencia
        if sorted(sequencia,reverse=True) == sequencia and len(sequencia)>len(menor):
            menor=sequencia
    for i in maior:
        print i,
    print
    for i in menor:
        print i,
        


    

    








