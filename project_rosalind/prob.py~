from math import log10

#with open('/home/augusto/Downloads/rosalind_prob.txt') as input_data:
#    dna, gc_content = input_data.readlines()

#Exemplo
dna='ACGATACAA'
gc_content='0.129 0.287 0.423 0.476 0.641 0.742 0.783'
        
gc_content = map(float, gc_content.split())

#Conta o n√∫mero de GC e AT
codon_count = [0, 0]
for codon in dna:
	if codon in ['C', 'G']:
		codon_count[0] += 1
	elif codon in ['A', 'T']:
		codon_count[1] += 1



gc_prob  = []
gc_prob2 = []
for gc_value in gc_content:
	log_prob = codon_count[0]*log10(0.5*gc_value) + codon_count[1]*log10(0.5*(1-gc_value))
        log_prob2 =log10((codon_count[0]/9.0*gc_value+codon_count[1]/9.0*(1-gc_value)))
	gc_prob.append(str(log_prob))
        gc_prob2.append(str(log_prob2))

print ' '.join(gc_prob)
print ' '.join(gc_prob2)
